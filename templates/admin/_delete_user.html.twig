<form method="post" action="{{ path('app_admin_delete_user', {'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this user?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <input type="submit" class="button-primary button-dark-red" value="Supprimer">
</form>